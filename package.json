{
	"name": "just-weather",
	"version": "0.0.1",
	"description": "Weather, just like it says...",
	"repository": "https://www.github.com/mgamlem3/just-weather",
	"author": "Michael Gamlem III, Tyler Gamlem",
	"license": "MIT",
	"private": false,
	"scripts": {
		"dev-client": "webpack serve",
		"dev-server": "nodemon -r dotenv/config --config nodemon.json --inspect server/server.ts",
		"prebuild": "yarn lint-check",
		"build": "rm -rf dist && webpack --mode production --config webpack.prod.config.js && webpack --mode production --config webpack.server.config.js",
		"test": "mocha ./tests/test.js --exit",
		"start": "node ./dist/server.*.wp.js",
		"prettier": "prettier --config ./.prettierrc --ignore-path  ./.prettierignore --write \"**/*.ts*\" \"**/*.js\" \"**/*.scss\" \"*.json\" \"*.md\"",
		"prettier-check": "prettier --list-different \"{*.js, *.md,./{src}/**/*.ts?(x)}\"",
		"lint-check": "yarn prettier-check && yarn eslint && yarn stylelint-check",
		"lint": "yarn prettier; yarn eslint; yarn stylelint",
		"eslint": "eslint '*/**/*.{js,ts,tsx}'",
		"stylelint": "stylelint --config ./.stylelintrc --fix \"**/*.scss\" --color --cache",
		"stylelint-check": "stylelint --config ./.stylelintrc \"**/*.scss\" --color --cache",
		"storybook": "start-storybook -p 6006 --no-dll",
		"build-storybook": "build-storybook --no-dll",
		"snapshot": "build-storybook && percy-storybook --widths=320,1280"
	},
	"browsersList": [
		"last 2 versions",
		"not dead",
		"> 0.2%"
	],
	"devDependencies": {
		"@babel/core": "^7.11.4",
		"@babel/plugin-transform-runtime": "^7.12.1",
		"@babel/preset-env": "^7.11.0",
		"@babel/preset-react": "^7.12.5",
		"@babel/preset-typescript": "^7.10.4",
		"@percy/storybook": "^3.3.1",
		"@storybook/addon-actions": "^6.0.28",
		"@storybook/addon-essentials": "^6.0.28",
		"@storybook/addon-links": "^6.0.28",
		"@storybook/preset-scss": "^1.0.3",
		"@storybook/react": "^6.0.28",
		"@types/express": "^4.17.9",
		"@types/node": "^14.14.7",
		"@types/react": "^16.9.56",
		"@types/react-dom": "^16.9.6",
		"@types/react-redux": "^7.1.12",
		"@types/react-router-dom": "^5.1.6",
		"@types/validator": "^13.1.1",
		"@typescript-eslint/eslint-plugin": "^4.7.0",
		"@typescript-eslint/parser": "^4.7.0",
		"@webpack-cli/serve": "^1.1.0",
		"babel-loader": "^8.2.1",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"chai": "^4.2.0",
		"compression-webpack-plugin": "^6.1.0",
		"css-loader": "^5.0.1",
		"eslint": "^7.13.0",
		"eslint-config-prettier": "^6.12.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.21.3",
		"file-loader": "^6.0.0",
		"html-webpack-plugin": "^4.4.1",
		"mini-css-extract-plugin": "^1.3.0",
		"mocha": "^8.2.1",
		"optimize-css-assets-webpack-plugin": "^5.0.4",
		"path": "^0.12.7",
		"prettier": "^2.1.2",
		"redux-devtools": "^3.7.0",
		"sass": "^1.29.0",
		"sass-loader": "^10.1.0",
		"style-loader": "^2.0.0",
		"stylelint": "^13.7.2",
		"stylelint-config-prettier": "^8.0.2",
		"stylelint-prettier": "^1.1.2",
		"supertest": "^6.0.1",
		"ts-node": "^9.1.0",
		"typescript": "^4.0.3",
		"webpack": "^4.44.2",
		"webpack-cli": "^4.2.0",
		"webpack-dev-server": "^3.11.0",
		"webpack-node-externals": "^2.5.2"
	},
	"dependencies": {
		"@reduxjs/toolkit": "^1.5.0",
		"axios": "^0.21.0",
		"body-parser": "^1.19.0",
		"bootstrap": "^4.6.0",
		"dotenv-webpack": "^6.0.0",
		"express": "^4.17.1",
		"express-validator": "^6.7.0",
